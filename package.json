{
  "name": "generative-art",
  "version": "0.0.1",
  "description": "A package for creating 'fractal' art on the fly",
  "main": "./lib/gArt.js",
  "engines": {
    "node": "6.5.0"
  },
  "scripts": {
    "pretest": "./node_modules/.bin/tsc",
    "test": "./node_modules/.bin/mocha",
    "test:watch": "./node_modules/.bin/mocha -w",
    "postinstall": "./node_modules/.bin/typings install"
  },
  "directories": {
    "test": "test",
    "lib": "lib"
  },
  "keywords": [
    "generative",
    "art",
    "fractal",
    "canvas",
    "graphic",
    "graphics",
    "pixman",
    "cairo",
    "image",
    "images",
    "png",
    "jpeg",
    "jpg",
    "gif"
  ],
  "author": {
    "name": "Matt Keeble",
    "email": "matt.keeble@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/beldougie/generative-art.git"
  },
  "bugs": {
    "url": "https://github.com/beldougie/generative-art/issues"
  },
  "license": "ISC",
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.0.2"
  },
  "dependencies": {
    "canvas": "https://github.com/beldougie/node-canvas.git",
    "lodash": "^4.15.0"
  }
}
