  },
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "chai": "^3.5.0",
    "mocha": "^3.0.2"
  },
  "dependencies": {
    "canvas": "^1.4.0",
    "lodash": "^4.15.0"
  }
